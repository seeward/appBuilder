{
  "name": "JS Playground",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "html/cs/js live editor",
  "homepage_url": "http://seeward.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
   "content_scripts": [
    {
      "matches":["file:///inject*"],
      
      "js": ["src/inject/inject.js"]
    }
  ],
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "bookmarks",
    "history"
  ]
}